{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"baseUrl": "./src"
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/kernel/prototypes/string.js", "src/kernel/prototypes/object.js", "src/kernel/prototypes/number.js", "src/kernel/prototypes/array.js"],
	"exclude": ["node_modules"]
}
